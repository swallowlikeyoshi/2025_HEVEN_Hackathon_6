<launch>
  <!-- Mission number (1, 2, 3) -->
  <arg name="map_number" default="1"/>
  <!-- Moving obstacle speed (Hz) -->
  <arg name="obs_speed" default="500"/>
    <!-- #########################수정함 11/14 정적장애물#############################/ -->
    <!-- Static Obstacle Number -->
  <arg name="map_obs_pos" default="1"/>
    <!-- #########################수정함 11/14 정적장애물#############################/ -->

  <!-- Launch a map from the maps folder -->
  <arg name="map" default="$(find racecar_simulator)/maps/parkingmap.yaml"/>
  <node pkg="map_server" name="map_server" type="map_server" args="$(arg map)"/>
  
  <!-- Launch the racecar model -->
  <include file="$(find racecar_simulator)/launch/racecar_model.launch"/>

  <!-- Begin the simulator with the parameters from params.yaml -->
  <node pkg="racecar_simulator" name="racecar_simulator" type="simulate" output="screen">
    <rosparam command="load" file="$(find racecar_simulator)/params.yaml"/>
  </node>

  <!-- Additional nodes -->
  <!-- Check collision -->
  <node pkg="racecar_simulator" name="check_collision" type="check_collision.py" output="screen">
    <param name="map_number" value="$(arg map_number)"/>
  </node>
  <!-- Pose publisher -->
  <node pkg="racecar_simulator" name="pose_pub" type="pose_pub.py" output="screen"/>
  <!-- Visualize objects -->
  <node pkg="racecar_simulator" name="visualize" type="visualize.py" output="screen">
    <param name="map_number" value="$(arg map_number)"/>
  </node>
  <!-- Mission node -->
  <node pkg="racecar_simulator" name="mission" type="mission.py" output="screen">
    <param name="map_number" value="$(arg map_number)"/>
  </node>

  <!-- Global Path Visualizer -->
  <node pkg="racecar_simulator" name="path_maker" type="pathmaker.py" output="screen"/>

  <!-- Obstacle generator -->
  <!-- <node pkg="racecar_simulator" name="obstacle_maker" type="obstaclemaker.py" output="screen"/> -->
  
  <!-- #########################수정함 11/14 정적장애물#############################/ -->
  <node pkg="racecar_simulator" name="static_obstacle_maker" type="static_obs_maker.py" output="screen">
    <param name="map_obs_pos" value="$(arg map_obs_pos)"/>
  </node>
  <!-- #########################수정함 11/14 정적장애물#############################/ -->

  <node pkg="racecar_simulator" name="moving_obstacle_maker" type="maker_moveobs.py" output="screen">
    <param name="obs_speed" value="$(arg obs_speed)"/>
  </node>

  <!-- Traffic generator -->
  <node pkg="racecar_simulator" name="traffic_gen" type="traffic_generator.py" output="screen"/>

  <!-- Rviz -->
  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find racecar_simulator)/rviz/racecar_rviz.rviz"/>
  
</launch>
